pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: 'main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/ajay5844/demo-merge.git']]])
            }
        }
        stage('checkout') {
            steps {
                echo "this is main branch job"
            }
        }
    }
}
